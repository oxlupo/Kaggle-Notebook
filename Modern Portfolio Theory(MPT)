     "iopub.status.busy": "2024-05-31T20:00:42.659093Z",
     "iopub.status.idle": "2024-05-31T20:00:42.715826Z",
     "shell.execute_reply": "2024-05-31T20:00:42.714101Z"
    },
    "papermill": {
     "duration": 0.101747,
